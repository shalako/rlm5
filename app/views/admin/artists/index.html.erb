<h1>Artists</h1>

<table>
<% @artists.each do |artist| %>
  <tr>
    <td><%=h artist.name %></td>
    <td><%= link_to 'Show', artist %></td>
    <td><%= link_to 'Edit', edit_admin_artist_path(artist) %></td>
    <td><%= link_to 'Destroy', [:admin, artist], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New artist', new_admin_artist_path %>
