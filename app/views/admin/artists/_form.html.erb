<% form_for [:admin, @artist], :html => {:multipart => true} do |f| %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.label :bio %><br />
    <%= f.text_area :bio %>
  </p>
  <p>
    <%= f.submit 'Submit' %> <%= link_to 'Cancel', admin_artists_path %>
  </p>
<% end %>
